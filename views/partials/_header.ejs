<header>

  <h1>TinyApp</h1>
  <% if (user) { %>
    Hello <%= user.email %>!
    <p>
        <form action="/logout" method="POST" style="margin-bottom: 50px">
        <input type="submit" value="Logout">
      </form>
    </p>
    <% } else { %>
      <p style="font-size:14px;">
         <a href="/login">Login</a><Br />
         <a href="/register">Register</a>
      </p>
      <% }%>
</header>
